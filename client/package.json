{
  "name": "portfolio",
  "description": "My personal website.",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npx eslint . --ext .js,.mjs,.cjs,.ts,.mts,.cts --cache && npx prettier --check .",
    "fix": "npx eslint . --ext .js,.mjs,.cjs,.ts,.mts,.cts --cache --fix && npx prettier --write . --ignore-unknown",
    "prepare": "is-ci || (cd .. && husky install client/.husky)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robere2/Portfolio.git"
  },
  "keywords": [
    "portfolio",
    "javascript",
    "vite",
    "frontend",
    "static"
  ],
  "author": "Erik Roberts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robere2/Portfolio/issues"
  },
  "homepage": "https://github.com/robere2/Portfolio#readme",
  "devDependencies": {
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "vite": "^4.5.12"
  },
  "dependencies": {
    "esbuild": "^0.14.39",
    "is-core-module": "^2.9.0",
    "has": "^1.0.3",
    "resolve": "^1.22.0",
    "rollup": "^2.74.1",
    "postcss": "^8.4.31",
    "function-bind": "^1.1.1",
    "path-parse": "^1.0.7",
    "source-map-js": "^1.0.2",
    "nanoid": "^3.3.8",
    "picocolors": "^1.0.0",
    "supports-preserve-symlinks-flag": "^1.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "lint-staged": {
    "**/*": "npm run fix"
  }
}
